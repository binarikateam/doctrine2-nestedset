        "phploc/phploc":    "*",
        "phpmd/phpmd":      "~2.2",
        "sebastian/phpcpd": "~2.0",

        "friendsofphp/php-cs-fixer": "*",

        "pdepend/pdepend":       "~2.1",
        "phpmetrics/phpmetrics": "~1.10"
    }
